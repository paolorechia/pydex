{
  "name": "pydex",
  "displayName": "pydex",
  "description": "Unofficial OpenAI API extension for Python",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pydex.addDocstring"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pydex.addDocstring",
        "title": "Add Docstring to Selection"
      },
      {
        "command": "pydex.addTypeHints",
        "title": "Add type hints to selection"
      },
      {
        "command": "pydex.fixSyntaxError",
        "title": "Fix syntax error"
      },
      {
        "command": "pydex.improveCodeQuality",
        "title": "Improve code quality"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "pydex.addDocstring",
          "when": "editorHasSelection"
        },
        {
          "command": "pydex.addTypeHints",
          "when": "editorHasSelection"
        },
        {
          "command": "pydex.fixSyntaxError",
          "when": "editorHasSelection"
        },
        {
          "command": "pydex.improveCodeQuality",
          "when": "editorHasSelection"
        }
      ],
      "editor/context": [
        {
          "command": "pydex.addDocstring",
          "when": "editorHasSelection",
          "group": "7_modification"
        },
        {
          "command": "pydex.addTypeHints",
          "when": "editorHasSelection",
          "group": "7_modification"
        },
        {
          "command": "pydex.fixSyntaxError",
          "when": "editorHasSelection",
          "group": "7_modification"
        },
        {
          "command": "pydex.improveCodeQuality",
          "when": "editorHasSelection",
          "group": "7_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "got": "^12.5.0"
  }
}